{
  "name": "trailguide",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon server/server.js",
    "build": "tsc -b && vite build",
    "start": "tsc -b && vite build && NODE_ENV=production node server/server.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "docker-remove-all": "docker rm $(docker ps -q -a -f 'name=trailguide-') --force && docker image rm $(docker images trailguide/trailguide-* -q) --force && docker volume rm $(docker volume ls -q -f 'name=trailguide*') --force"
  },
  "dependencies": {
    "@aws-sdk/client-cloudtrail": "^3.583.0",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "pg": "^8.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "react-simple-maps": "^3.0.0",
    "recharts": "^2.13.0",
    "vite-express": "^0.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@types/react-simple-maps": "^3.0.6",
    "@types/recharts": "^1.8.29",
    "@vitejs/plugin-react": "^4.3.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.11.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
